<UserControl x:Class="Boxer.Controls.ImageViewer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:controls="clr-namespace:Boxer.Controls"
             xmlns:winForms="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
             xmlns:spriteUtility="clr-namespace:SpriteUtility"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <WindowsFormsHost>
        <spriteUtility:ImageViewer x:Name="imageViewer"></spriteUtility:ImageViewer>
    </WindowsFormsHost>
</UserControl>
